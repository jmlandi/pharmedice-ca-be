# Option 1: Resend (Recommended)
MAIL_MAILER=resend
RESEND_API_KEY=your_resend_api_key_here
MAIL_FROM_ADDRESS="nao-responda@marcoslandi.com"
MAIL_FROM_NAME="Pharmedice"

